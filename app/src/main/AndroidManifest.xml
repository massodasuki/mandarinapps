<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unifreelancer.masso.learnmandarin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Video"
            android:label="@string/title_activity_video"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Challenge"
            android:label="@string/title_activity_challenge"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NewChallenge" />
        <activity android:name=".NewChallenge1" />
        <activity android:name=".NewChallenge2" />
        <activity android:name=".Complete" />
        <activity android:name=".Leaderboard"></activity>
    </application>

</manifest>